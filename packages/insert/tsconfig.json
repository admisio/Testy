{
	"compilerOptions": {
		"target": "ES6",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"isolatedModules": false,
		"moduleResolution": "Node"
	}
}
